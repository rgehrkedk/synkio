{
  "name": "synkio-plugin-import",
  "version": "1.0.0",
  "description": "Synkio Import - Import JSON design tokens as Figma Variables",
  "private": true,
  "scripts": {
    "build": "esbuild src/code.ts --bundle --outfile=code.js --target=es2020 && esbuild src/ui.tsx --bundle --outfile=ui.js --target=es2020",
